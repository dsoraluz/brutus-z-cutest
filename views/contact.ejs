      <form action="/contact" method="post">
        <div class="input-field">
          <label for="name">What is your name? *</label>
          <input id="name" type="text" class="validate" name="name"/>
        </div>
        <div class="input-field">
          <label for="email">What is your email? *</label>
          <input id="email" type="text" class="validate" name="email"/>

        </div>
        <div class="input-field">
          <label for="message">Throw me a bone. *</label>
          <input id="message" type="text" class="validate" name="name" placeholder="How can we collaborate?"/>

        </div>
        <button id="send-button" class="btn waves-effect #424242 grey darken-3" type="submit">Send Message</button>

      </form>

    </div>
    <div class="col s6">
      <div class="picture">
        <img src="./images/brutus-sitting.jpg" />

      </div>
    </div>
